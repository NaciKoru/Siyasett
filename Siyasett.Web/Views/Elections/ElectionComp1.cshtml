        <div class="inner-container clearfix">
            <div class="title-box">
                <h1>@ViewData["Title"]</h1>

            </div>
            <div class="bread-crumb">

                <ul>
                    <li><a href="/"><i class="fa fa-home"></i></a></li>
                    <li><a href="/secimler">@Localize("page_elections_meta_title")</a></li>
