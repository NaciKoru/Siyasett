        <div class="card">
            <div class="card-header">
                <div class="sec-title centered mt-4 mb-2">
                    <h3 class="mb-3"> @Localize("page_elections_electiondetail1_distres")</h3>
                    <div class="separater"></div>


                </div>
            </div>
            <div class="card-body">
                <table class="table table-striped">
                    <thead style="position:sticky;top: 75px">
                        <tr style="background-color:white">
                            <th>@Localize("page_elections_electiondetail1_district")</th>
                            @{
                                var count = 0;
                            }
                            @foreach (var item in parties)
                            {
                                if (count < 13)
                                {
                                    var sira = count + 1;
                                    if (@item.PartyNameShort.Contains("Bağımsız"))
                                    {
                                        <th style="text-align:center" id="partiThid+@sira" onclick="sirala(@sira,1)">@(cultureName == "tr-TR" ? item.PartyNameShort : "Independents")</th>

                                    }
                                    else
                                    {
                                        <th style="text-align:center" id="partiThid+@sira" onclick="sirala(@sira,1)">@item.PartyNameShort</th>

                                    }
                                    count++;
                                }

                            }
                        </tr>
                    </thead>
                    <tbody id="tblBody">

                        @foreach (var item in secimIlceler)
                        {

                            <tr>
                                @{
                                    if (il.Name != il2.Name)
                                    {
                                        <td><a asp-route="secim-mah-detay" asp-route-cevreid="@item.RegionId" asp-route-secim="@secim.Id" asp-route-id="@item.Id" asp-route-ilId="@il.Id">@item.Name</a></td>
                                    }
                                    else
                                    {
                                        <td><a asp-route="secim-mah-detay" asp-route-cevreid="@item.RegionId" asp-route-secim="@secim.Id" asp-route-id="@item.Id" asp-route-ilId="@il.Id" asp-route-cevreid2="@secimIlceler2.FirstOrDefault(a => a.Name == item.Name).RegionId" asp-route-secim2="@secim2.Id" asp-route-id2="@secimIlceler2.FirstOrDefault(a => a.Name == item.Name).Id" asp-route-ilId2="@il2.Id">@item.Name</a></td>
                                    }


                                    count = 0;
                                }
                                @foreach (var party in parties)
                                {
                                    if (count < 13)
                                    {
                                        <td style="text-align:center">
                                            @{
                                                var sonuc = secimSonuclar.Where(i => i.PartyId == party.Id && i.IlceId == item.Id && i.YskSecimCevresiId == item.RegionId).FirstOrDefault();
                                                var sonuc2 = new ElectionPartyResultModel();
                                                if (secimIlceler2.FirstOrDefault(a => a.Name == item.Name) != null && sonuc2 != null)
                                                {
                                                    if (secimSonuclar2.Where(i => secimIlceler2.FirstOrDefault(a => a.Id == i.IlceId).Name == item.Name) != null)
                                                    {
                                                        sonuc2 = secimSonuclar2.Where(i => i.PartyId == party.Id && secimIlceler2.FirstOrDefault(a => a.Id == i.IlceId).Name == item.Name && i.YskSecimCevresiId == secimIlceler2.FirstOrDefault(a => a.Name == item.Name).RegionId).FirstOrDefault();

                                                    }
                                                }
                                            }
                                            @if (sonuc != null)
                                            {
                                                <span>@sonuc.Vote.Value.ToString("#,##0") @Html.Raw(" ")</span>
                                                if (sonuc2 != null && sonuc2.Vote != null)
                                                {
                                                    <span>@sonuc2.Vote.Value.ToString("#,##0")</span>
                                                }
                                                <br />
                                                var oran1 = @Math.Round((100 * Convert.ToDouble(sonuc.Vote.Value) / Convert.ToDouble(secimSonuclar.Where(i => i.IlceId == item.Id && i.YskSecimCevresiId == item.RegionId).Select(i => i.Vote).Sum())), 2);
                                                <span>@yuzde@oran1 @Html.Raw(" ")</span>
                                                if (sonuc2 != null)
                                                {
                                                    if (sonuc2 != null && sonuc2.Vote != null)
                                                    {
                                                        var oran2 = @Math.Round((100 * Convert.ToDouble(sonuc2.Vote.Value) / Convert.ToDouble(secimSonuclar2.Where(i => secimIlceler2.FirstOrDefault(a => a.Id == i.IlceId).Name == item.Name && i.YskSecimCevresiId == secimIlceler2.FirstOrDefault(a => a.Name == item.Name).RegionId).Select(i => i.Vote).Sum())), 2);
                                                        <span>@yuzde@oran2</span>

                                                    }

                                                }
                                            }
                                            else
                                            {
                                                <span>-</span>
                                            }
                                        </td>
                                        count++;
                                    }
                                }
                            </tr>


                        }
                    </tbody>
                </table>
                </>
            </div>






        </div>
</section>
@section scripts{
    <link href="~/lib/apexcharts/apexcharts.min.css" rel="stylesheet" />
    <script src="~/lib/apexcharts/apexcharts.min.js"></script>
    <script src="~/js/election-listing.js" asp-append-version="true"></script>

    <script>

        $(function () {

            createPosition();
            createIdeology();



            $('#tblBody').find('tr').each((i, el) => {
                var max = 0;
                var maxEl;
                $(el).find('td').each((v, vel) => {
                    if (v > 0) {
                        var f = parseFloat($(vel).find(">:first-child").html().replace(',', '').replace('.', ''));
                        if (!isNaN(f) && f > max) {
                            max = f;
                            maxEl = vel;
                        }
                    }

                });
                if (maxEl) {
                    $(maxEl).css("font-weight", "bold");
                }
                $(el).find('td').each((v, vel) => {
                    if (v > 0) {

                        var f1 = parseFloat($(vel).children().eq(0).text().replace(',', '').replace('.', ''));
                        var f2 = parseFloat($(vel).children().eq(1).text().replace(',', '').replace('.', ''));
                        if (!isNaN(f2)) {
                            var f4 = parseFloat($(vel).children().eq(4).text().replace('%', '').replace(',', '.')).toFixed(2);
                            var f3 = parseFloat($(vel).children().eq(3).text().replace('%', '').replace(',', '.')).toFixed(2);

                            var newtext = f1.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1.") + ' <span >' + f2.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1.") + '</span>';
                            if ((f4 - f3) > 0) {
                                newtext += '</br> % ' + f3.toString().replace('.', ',') + ' <span style="color:#8bd14a">% ' + f4.toString().replace('.', ',') + '</span><span style="display:none" id="asd' + v + 'asd">' + (f4 - f3) + '</span>';
                            }
                            else {
                                newtext += '</br> % ' + f3.toString().replace('.', ',') + ' <span style="color:#d92e10">% ' + f4.toString().replace('.', ',') + '</span><span style="display:none" id="asd' + v + 'asd">' + (f4 - f3) + '</span>';
                            }
                            $(vel).html(newtext);

                        } else {
                            var td = '<span style="display:none" id="asd' + v + 'asd">' + 0 + '</span>'
                            $(vel).append(td);
                        }



                    }
                });
            });
        });


        var barHeight = 40;
        function makeChart(datas, categories, xtitle, ytitle, divname) {

            var options = {
                series: [{
                    data: datas,
                    name: " ",
                }],
                chart: {
                    type: 'bar',

                    height: 60 + datas.length * barHeight
                },
                plotOptions: {
                    bar: {
                        borderRadius: 4,
                        horizontal: true,
                        distributed: true,
                        dataLabels: {
                            position: 'top',
                        },
                    }
                },
                colors: ['#33b2df', '#546E7A', '#d4526e', '#13d8aa', '#A5978B', '#2b908f', '#f9a3a4', '#90ee7e',
                    '#f48024', '#69d2e7'
                ],
                dataLabels: {
                    enabled: true,
                    textAnchor: 'start',
                    style: {
                        fontSize: '11px',
                        fontFamily: 'Helvetica, Arial, sans-serif',
                        fontWeight: 'bold',
                        colors: '#000000'
                    },
                },
                xaxis: {
                    categories: categories,
                    title: {
                        text: xtitle,
                    },
                },
                yaxis: {
                    title: {
                        text: ytitle,
                    },
                },
                labels: categories
            };

            var chart = new ApexCharts(document.querySelector(divname), options);
            chart.render();
        };


        function makePieChart(datas, categories, divname) {

            var options = {
                series: datas,
                chart: {
                    height: 350,
                    type: 'pie',
                },
                legend: {
                    show: true,
                    position: 'bottom',
                    floating: false,
                    verticalAlign: 'bottom',
                    align: 'center'
                },
                labels: categories
            };

            var chart = new ApexCharts(document.querySelector(divname), options);
            chart.render();
        };

        function createIlChart() {

            var secimdetayid = @secimDetay.Id;
            var secimilid = @il.Id;
            var secimilceid = 0;

            $.get('/elections/GetSecimIlChartData?secimdetay=' + secimdetayid + '&secimil=' + secimilid)
                .done(function (response) {
                    var data = response.data;
                    var options = {
                        series: [{
                            data: [],
                            name: "Oy ",
                        }],
                        chart: {
                            type: 'bar',
                            height: 30 + data.length * barHeight
                        },
                        plotOptions: {
                            bar: {
                                borderRadius: 3,
                                horizontal: true,
                                distributed: true,
                                dataLabels: {
                                    position: 'top',
                                },
                            }
                        },
                        colors: ['#33b2df', '#546E7A', '#d4526e', '#13d8aa', '#A5978B', '#2b908f', '#f9a3a4', '#90ee7e',
                            '#f48024', '#69d2e7', '#9ec54b', '#ff3d7f', '#e8caaf', '#fc9d9a'
                        ],
                        dataLabels: {
                            enabled: true,
                            textAnchor: 'start',
                            style: {
                                fontSize: '11px',
                                fontFamily: 'Helvetica, Arial, sans-serif',
                                fontWeight: 'bold',
                                colors: '#000000'
                            },
                            formatter: function (val, opts) {
                                console.log(opts);
                                var total = 0;
                                for (var i = 0; i < opts.w.globals.series[opts.seriesIndex].length; i++) {
                                    total += opts.w.globals.series[opts.seriesIndex][i];
                                }
                                console.log(total);
                                let percent = (100 * val) / total;
                                var vstr = new Intl.NumberFormat('tr-TR').format(val);
                                return vstr + ' (' + percent.toFixed(1) + '%)'

                            }
                        },
                        xaxis: {
                            categories: [],
                            title: {
                                text: '@cultureName' === "tr-TR" ? "Oy Sayısı" : "Total Votes",
                            },

                        },
                        yaxis: {
                            title: {
                                text: '@cultureName' === "tr-TR" ? "Partiler" : "Parties",
                            },
                        },
                    };

                    for (var i = 0; i < data.length; i++) {
                        if (i < 7) {
                            options.series[0].data.push(data[i].ratioAvg);
                            //options.colors.push(getColor(i));
                            options.xaxis.categories.push(data[i].partyNameShort);
                        }

                    }
                    var chart = new ApexCharts(document.querySelector("#chartparties"), options);
                    chart.render();
                });
        }

        function createPosition() {

            var secimdetayid = @secimDetay.Id;
            var secimilid = @il.Id;

            $.get('/elections/GetSecimIlPositionsChartData?secimdetay=' + secimdetayid + '&secimil=' + secimilid)
